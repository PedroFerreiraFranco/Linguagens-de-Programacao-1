#include <stdio.h>
#include <stdlib.h>

int main(){
	int vet[10],i,n;
	
		printf("Quantidade de elementos do vetor: ");
	scanf("%d",&n);
	
	for(i=0;i<n;i++){
		printf("Vetor[%d]:",i);
		scanf("%d",&vet[i]);
	}	
	
		printf("\n*Colocando em ordem invesa*\n");
			for(i=n-1;i>=0;i--){
				printf("\nVetor[%d] = %d",i,vet[i]);
			}
		}
